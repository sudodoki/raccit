{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "no-undef": [1],
    "key-spacing": [0, {
      "beforeColon": true,
      "afterColon": true
    }],
    "import/no-extraneous-dependencies": 0,
    "no-unused-vars": ["error", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }],
    "padded-blocks": 0,
    "no-else-return": 0,
    "space-before-function-paren": [1, "never"],
    "max-len": [2, 120, 2, {
      "ignoreComments": true,
      "ignoreUrls": true
    }]
  }
}
